{
    "name": "project/project",
    "description": "Project management package for Laravel",
    "version": "1.0.0",
    
    "autoload": {
        "psr-4": {
            "Project\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Project\\ProjectServiceProvider"
            ],
            "aliases": {
                "Project": "Project\\Facades\\Project"
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/project"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Project\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
